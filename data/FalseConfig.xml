<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<HistPlotter>
	<include name='setup1' value='test/canv.xml' />
	<include name='setup2' value='test/canv1.xml' />
	<include name='setup2' value='test/canv3.xml' />
	<source>
		<input name="SemiLepAnalysis">
			<folder name='analysis'>
				<filter type='contains' value='muon' />
			</folder>
			<folder name='kinematic'>
			<!-- multiple filters? -->
				<filter type='contains' value='pt' />
				<filter type='exact' value='METdistribution' />
			</folder>
		</input>
		<input name='kinematicPlots'>
			<folder name='kinematic'>
				<filter type='contains' value='elec' />
			</folder>
		</input>
	<!-- includes of the data -->

	</source>
	<!--
		makes the canvas, mixes the canvogramms defined in the canvSetup
		summaryFile = ps oder pdf
	-->
	<plots create='eps,pdf' makeSummaryFile='true' summaryFile="inspect.pdf">
		<canv name='muonPt' defaults='HistSetupName'>
		<!-- name is also used for legend -->
			<hist name='qcd' from='file:qcd' markerStyle='22' color='23' />
			<hist name='signal' from='file:top' markerStyle='23' color='blue' />
			<hist name='combined' from='hist:signal,hist:qcd' compute='add'
				markerStyle='24' />
			<legend name="plotname" posx="20" posy="0" fonsize="12"/>
		</canv>
		<canvlist name='electronKinematic' input='kinematicPlots'>
			<hist name='qcd' from='file:qcd' markerStyle='22' color='23' />
			<hist name='signal' from='file:top' markerStyle='23' color='blue' />
		</canvlist>
		<canvlist name='AllHistos'>
			<hist name='qcd' from='file:qcd' markerStyle='22' color='23' />
			<hist name='signal' from='file:top' markerStyle='23' color='blue' />
		</canvlist>
	</plots>
</HistPlotter>