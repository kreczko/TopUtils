<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- Works with LKThesis.xml -->
<HistSetup>
	<hist name='jet1_et' input='analyzeSelJets/kin_e_0'>
		<var name='QCD(#mu enriched)' source='file:qcd' type='line' style='1' color='2' norm='Integral' />
		<var name='Top(signal)' source='file:top' type='line' style='1' color='4' norm='Integral' />
		<var name='W + jets' source='file:wjets' type='line' style='1' color='8'
			norm='Integral' />
		<titleX v='E_{T}(jet 1) [GeV]' />
		<titleY v='normalized to unit area' />
		<savefolder v='kin' />
		<legend posX="45" sizeX="50" sizeY="40" posY="45"/>
	</hist>
