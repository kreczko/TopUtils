		<titleY v='normalized to unit area' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
	</canv>

	<canv name='MET_reso' input='analyzeisolationMET/hist_recoMETGenMET'>
		<hist name="QCD(#mu enriched)" source='file:qcd' color='2' norm='Integral' />
		<titleX v='MET_{reco} - MET_{gen} [GeV]' />
		<titleY v='normalized to unit area' />
		<savefolder v='analysis' />
	</canv>
	
	<canv name='MET_reso_Top' input='analyzeisolationMET/hist_recoMETGenMET'>
		<hist name="Top(Signal)" source='file:top' norm='Integral' />
		<titleX v='MET_{reco} - MET_{gen} [GeV]' />
		<titleY v='normalized to unit area' />
		<savefolder v='analysis' />
	</canv>
	
	<canv name='MET_reso_Wjets' input='analyzeisolationMET/hist_recoMETGenMET'>
