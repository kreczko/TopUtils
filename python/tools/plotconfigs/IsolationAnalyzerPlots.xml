		<titleY v='#Sigma_{\Delta R 0.3} p_{T}(tracks)[GeV]' />
		<titleX v='\Delta\phi(missing E_T, muon)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend />
	</hist>

	<hist name='corr_dPhiMETMuVSjetIso_qcd' input='analyzeisolationMET/corr_deltaPhiMetleadingMuonVSjetiso'>
		<!-- name is also used for legend -->
		<var name='wjets' source='file:qcd' color='2' />
		<titleY v='distance to closest jet' />
		<titleX v='\Delta\phi(MET, \mu)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend />
	</hist>



	<hist name='norm_MET' input='analyzeisolationMET/var_norm_MET'>
		<!-- name is also used for legend -->
		<var name='qcdMuEnriched' source='file:qcd' color='2' />
		<var name='top' source='file:top' color='4' draw='0' />
		<var name='wjets' source='file:wjets' color='8' draw='0' />
		<var name='signal' source='var:top,var:wjets' operation='add' color='4'
			norm='Integral' size="3" />
		<titleX v='MET [GeV]' />
		<titleY v='normalized to unit area' />
		<savefolder v='analysis' />
		<legend posX="35" sizeX="50" posY="40" sizeY="10" />
	</hist>

	<hist name='corr_METVStrack_qcd' input='analyzeisolationMET/corr_METVStrack'>
		<var name='wjets' source='file:qcd' color='2' />
		<titleY v='#Sigma_{\Delta R 0.3} p_{T}(tracks) [GeV]' />
		<titleX v='\Delta\phi(MET, \mu)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend />
