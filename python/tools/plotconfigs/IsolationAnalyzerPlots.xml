<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- Works with LKThesis.xml -->
<HistSetup>
	<canv name='MVAdisc_signal' input='analyzeisolationMET/hist_MVAdisc'>
		<hist name="QCD(#mu enriched)" source='file:qcd' color='2' draw='0' />
		<hist name='top' source='file:top' color='4' />
		<hist name='wjets' source='file:wjets' color='8' />
		<titleX v='d_{MVA}' />
		<savefolder v='analysis' />
		<legend posX='40' />
	</canv>
	<canv name='MVAdisc_qcd' input='analyzeisolationMET/hist_MVAdisc'>
		<hist name="QCD(#mu enriched)" source='file:qcd' color='2' />
		<hist name='top' source='file:top' color='4' />
		<hist name='wjets' source='file:wjets' color='8' />
		<titleX v='d_{MVA}' />
		<savefolder v='analysis' />
		<legend posX='40' />
	</canv>
	
	<canv name='deltaPhiJet1Jet2' input='analyzeisolationMET/hist_deltaPhiJet1Jet2'>
		<hist name='QCD(#mu enriched)' source='file:qcd' color='2' norm='Integral' />
		<hist name='Top(signal)' source='file:top' color='4' norm='Integral' />
		<hist name='W+jets' source='file:wjets' color='8' norm='Integral' />
		<titleX v='#Delta#phi(jet 1, jet 2)' />
		<titleY v="normalized to unit area"/>
		<savefolder v='analysis' />
		<legend posX='35' sizeX="45" posY="50" />
	</canv>

	<canv name='norm_MVAdisc' input='analyzeisolationMET/hist_MVAdisc'>
		<hist name='QCD(#mu enriched)' source='file:qcd' color='2' norm='Integral' />
		<hist name='Top(signal)' source='file:top' color='4' norm='Integral' />
		<hist name='W+jets' source='file:wjets' color='8' norm='Integral' />
		<titleX v='d_{MVA}' />
		<titleY v='normalized to unit area' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
		<legend posX='25' sizeX="55" />
	</canv>

	<canv name='norm_MVAdisc_OL' input='analyzeisolationMET/hist_MVAdisc'>
