<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- Works with LKThesis.xml -->
<HistSetup>
	<hist name='MVAdisc_signal' input='analyzeisolationMET/var_MVAdisc'>
		<var name='qcd' source='file:qcd' color='2' draw='0' />
		<var name='top' source='file:top' color='4' />
		<var name='wjets' source='file:wjets' color='8' />
		<titleX v='MVA disc.' />
		<savefolder v='analysis' />
		<legend posX='40' />
	</hist>
	<hist name='MVAdisc_qcd' input='analyzeisolationMET/var_MVAdisc'>
		<var name='qcd' source='file:qcd' color='2' />
		<var name='top' source='file:top' color='4' />
		<var name='wjets' source='file:wjets' color='8' />
		<titleX v='MVA disc.' />
		<savefolder v='analysis' />
		<legend posX='40' />
	</hist>

	<hist name='norm_MVAdisc' input='analyzeisolationMET/var_MVAdisc'>
		<var name='qcdMuEnriched' source='file:qcd' color='2' norm='Integral' />
		<var name='top-signal' source='file:top' color='4' norm='Integral' />
		<var name='W+jets' source='file:wjets' color='8' norm='Integral' />
		<titleX v='MVA disc.' />
		<titleY v='normalized to unit area' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
		<legend posX='25' sizeX="55" posY="35" />
	</hist>

	<hist name='norm_MVAdisc_OL' input='analyzeisolationMET/var_MVAdisc'>
		<var name='qcdMuEnriched' source='file:qcd' color='2' norm='Integral' />
		<var name='top' source='file:top' color='4' norm='Integral' />
		<var name='wjets' source='file:wjets' color='8' norm='Integral' />
		<titleX v='MVA disc.' />
		<titleY v='normalized to unit area' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
	</hist>

	<hist name='MET_reso' input='analyzeisolationMET/var_recoMETGenMET'>
		<var name='qcd' source='file:qcd' color='2' norm='Integral' />
		<titleX v='MET_{reco} - MET_{gen} [GeV]' />
		<titleY v='normalized to unit area' />
		<savefolder v='analysis' />
		<!--		<legend posX='20' />-->
	</hist>

	<hist name='var_norm_dPhiMuJ12' input='analyzeisolationMET/var_norm_DeltaPhiMuonJet12'>
		<var name='qcd' source='file:qcd' color='2' />
		<var name='top' source='file:top' color='4' />
		<var name='wjets' source='file:wjets' color='8' />
		<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
		<titleY v='normalized to unit area' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
		<!--		<legend posX='40' />-->
	</hist>

	<hist name='corr_DphiMuJ1J2VScalo_qcd' input='analyzeisolationMET/corr_DeltaPhiMuonJet12VScalo'>
		<!-- name is also used for legend -->
		<var name='wjets' source='file:qcd' color='2' />
		<titleY v='\Sigma_{\Delta R 0.3} E_{T}(calotower) [GeV]' />
		<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend />
	</hist>

	<hist name='corr_DphiMuJ1J2VStrack_qcd' input='analyzeisolationMET/corr_DeltaPhiMuonJet12VStrack'>
		<!-- name is also used for legend -->
		<var name='wjets' source='file:qcd' color='2' />
		<titleY v='#Sigma_{\Delta R 0.3} p_{T}(tracks)[GeV]' />
		<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend />
	</hist>

	<hist name='corr_DphiMuJ1J2VSjetIso_qcd' input='analyzeisolationMET/corr_DeltaPhiMuonJet12VSjetiso'>
		<!-- name is also used for legend -->
		<var name='wjets' source='file:qcd' color='2' />
		<titleY v='distance to closest jet' />
		<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend />
	</hist>

	<hist name='var_norm_dPhiMETMu' input='analyzeisolationMET/var_norm_deltaPhiMetleadingMuon'>
		<var name='qcd' source='file:qcd' color='2' />
		<var name='top' source='file:top' color='4' />
		<var name='wjets' source='file:wjets' color='8' />
		<titleX v='\Delta\phi(MET, \mu)' />
		<titleY v='normalized to unit area' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
		<!--		<legend posX='40' />-->
	</hist>

	<hist name='corr_dPhiMETMuVScalo_qcd' input='analyzeisolationMET/corr_deltaPhiMetleadingMuonVScalo'>
		<!-- name is also used for legend -->
		<var name='wjets' source='file:qcd' color='2' />
		<titleY v='#Sigma_{\Delta R 0.3} E_{T}(calotower)[GeV]' />
		<titleX v='\Delta\phi(missing E_T, muon)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend />
	</hist>

	<hist name='corr_dPhiMETMuVStrack_qcd' input='analyzeisolationMET/corr_deltaPhiMetleadingMuonVStrack'>
		<!-- name is also used for legend -->
