<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- Works with LKThesis.xml -->
<HistSetup>
	<!-- From MuonAnalyzer -->
	<canv name='muon_pt' input='analyzeSelMuons/kin_pt'>
		<!-- name is also used for legend -->
		<hist name='qcd' source='file:qcd' color='2' norm='Integral' />
		<hist name='top' source='file:top' color='4' norm='Integral' />
		<hist name='wjets' source='file:wjets' color='8' norm='Integral' />
		<titleX v='p_{T}(#mu) [GeV]' />
		<titleY v='normalized to unit area' />
		<savefolder v='kin' />
		<!--		<legend />-->
	</canv>
	
	<canv name='muon_pt2' input='analyzeSelMuons/kin_pt'>
		<!-- name is also used for legend -->
		<hist name='QCD(#mu enriched)' source='file:qcd' color='2' norm='Integral' />
		<hist name='Top(signal)' source='file:top' color='4' norm='Integral' />
		<hist name='W + jets' source='file:wjets' color='8' norm='Integral' />
		<titleX v='p_{T}(#mu) [GeV]' />
		<titleY v='normalized to unit area' />
		<savefolder v='kin' />
		<legend posX="37" sizeX="60" posY="35" sizeY="50"/>
	</canv>
	
	<canv name='muon_calIso' input='analyzeSelMuons/iso_isoCalPt'>
		<!-- name is also used for legend -->
		<hist name='QCD(#mu enriched)' source='file:qcd' color='2' norm='Integral' />
		<hist name='Top(signal)' source='file:top' color='4' norm='Integral' />
		<hist name='W + jets' source='file:wjets' color='8' norm='Integral' />
		<titleX v='E_{T}^{weighted} [GeV]' />
		<titleY v='normalized to unit area' />
		<savefolder v='iso' />
		<legend posX="25" sizeX="65" />
	</canv>

	<canv name='muon_calomix'>
		<hist name='qcdE' source='file:qcd, input:ecalIsoPt' color='2' draw='0' />
		<hist name='topE' source='file:top, input:ecalIsoPt' color='4' draw='0' />
		<hist name='wjetsE' source='file:wjets, input:ecalIsoPt' color='8' draw='0' />
		<hist name='qcdH' source='file:qcd, input:hcalIsoPt' color='2' draw='0' />
		<hist name='topH' source='file:top, input:hcalIsoPt' color='4' draw='0' />
		<hist name='wjetsH' source='file:wjets, input:hcalIsoPt' color='8' draw='0' />

		<hist name='qcd' source='hist:qcdE, hist:qcdH' color='2' 
			operation='add' />
		<hist name='top' source='hist:topE, hist:topH' color='4' 
			operation='add' />
		<hist name='wjets' source='hist:wjetsH, hist:wjetsE' color='8' 
			operation='add' />
		<titleX v=' #Delta R(#mu, tower)' />
		<titleY v='#Sigma E_{T}(tower) per #mu' />
		<maxX v='1' />
		<minX v='0' />
		<savefolder v='iso' />
<!--		<legend posX='25' />-->
	</canv>

	<canv name='muon_trackIso' input='analyzeSelMuons/iso_isoTrkPt'>
		<!-- name is also used for legend -->
		<hist name='QCD(#mu enriched)' source='file:qcd' color='2' norm='Integral' />
		<hist name='Top(signal)' source='file:top' color='4' norm='Integral' />
		<hist name='W + jets' source='file:wjets' color='8' norm='Integral' />
		<titleX v="#Sigma_{#Delta R &lt; 0.3} p_{T}(track) [GeV]" />
		<titleY v='normalized to unit area' />
		<savefolder v='iso' />
		<legend posX='25' sizeX="65"/>
	</canv>

	<canv name='muon_PtTracksVSdR' input='analyzeSelMuons/iso_dRTrkPt'>
