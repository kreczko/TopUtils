		<hist name='QCD(#mu enriched)' source='file:qcd' color='2' />
		<hist name='Top(signal)' source='file:top' color='4' />
		<hist name='W + jets' source='file:wjets' color='8' />
		<titleX v='#Delta R(#mu, track)' />
		<titleY v='#Sigma p_{T} of tracks within #Delta R' />
		<savefolder v='iso' />
		<legend posX='20' />
	</canv>
	<canv name='muon_NTracksVSdR' input='analyzeSelMuons/iso_dRTrkN'>
		<hist name='qcd' source='file:qcd' color='2' />
		<hist name='top' source='file:top' color='4' />
		<hist name='wjets' source='file:wjets' color='8' />
		<titleX v='#Delta R(#mu, track)' />
		<titleY v='# of tracks within #Delta R per #mu' />
		<savefolder v='iso' />
		<maxX v="1" />
		<minX v="0" />
<!--		<legend posX='20' />-->
	</canv>

