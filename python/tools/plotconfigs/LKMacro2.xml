<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- rule declaration possible -->
	<!-- http://www.devmag.net/xml/xml_schema_elemente_attribute.htm -->
	<!-- final steering file -->
<HistPlotter>
	<sources>
		<!-- gets input from 1 histogram -->
		<input name='qcd20'>
			<folder name="qcd/jetcut_20" />
		</input>
		<file id="qcd" name="/playground/rootfiles/FINAL/qcdmu_20j20m.root" />
		<file id="qcdCircSIG" name="/playground/rootfiles/FINAL/shapes/qcdMu_CircSIG.root" />
		<file id="qcdCircBG" name="/playground/rootfiles/FINAL/shapes/qcdMu_CircBG.root" />

		<file id="qcdPtSIG" name="/playground/rootfiles/FINAL/shapes/qcdMu_PtSIG.root" />
		<file id="qcdPtBG" name="/playground/rootfiles/FINAL/shapes/qcdMu_PtBG.root" />

		<file id="qcdMJ12SIG" name="/playground/rootfiles/FINAL/shapes/qcdMu_MJ12SIG.root" />
		<file id="qcdMJ12BG" name="/playground/rootfiles/FINAL/shapes/qcdMu_MJ12BG.root" />

		<file id="qcdMETMuSIG" name="/playground/rootfiles/FINAL/shapes/qcdMu_METMuSIG.root" />
		<file id="qcdMETMuBG" name="/playground/rootfiles/FINAL/shapes/qcdMu_METMuBG.root" />

		<file id="macro" name="/playground/rootfiles/FINAL/complete_hists_MM.root" />
	</sources>
	<plots create='png' makeSummaryFile='true' summaryFile='inspect.pdf'>
		<!-- Macro plots -->
		<hist name='weights'>
			<var name='sig.-like' source='file:macro' input="shapes/circSIG" color='2' />
			<var name='bg-like' source='file:macro' input="shapes/circBG" color='4' />
			<titleX v='circ' />
			<titleY v='weigth factor' />
			<savefolder v="shapes/circ" />
			<legend posX="55" posY="40"/>
		</hist>
		<hist name='quality_tight' >
			<var name='qcd' source='file:macro' input="applied/jetcut_20/all/bkg/quality_tight" size="3"/>
			<var name='qcdBG' source='file:macro' input="shapes/circularitySIG/all/bkg/quality_tight" color='2' drawOption="e4" type="filled" style="3005"/>
			<var name='qcdSIG' source='file:macro' input="shapes/circularityBG/all/bkg/quality_tight" color='4' drawOption="e4" type="filled" style="3004"/>
			<titleX v='MVA disc. cut' />
			<titleY v='N_{est}/N_{true}' />
			<savefolder v="shapes/circ" />
			<maxY v="1.55" />
			<minY v="0.55" />
		</hist>
		
		<hist name='quality_loose' >
			<var name='qcd' source='file:macro' input="applied/jetcut_20/all/bkg/quality_loose" size="3"/>
			<var name='qcdBG' source='file:macro' input="shapes/circularitySIG/all/bkg/quality_loose" color='2' drawOption="e4" type="filled" style="3005"/>
			<var name='qcdSIG' source='file:macro' input="shapes/circularityBG/all/bkg/quality_loose" color='4' drawOption="e4" type="filled" style="3004"/>
			<titleX v='MVA disc. cut' />
			<titleY v='N_{est}/N_{true}' />
			<savefolder v="shapes/circ" />
			<maxY v="1.016" />
			<minY v="0.992" />
		</hist>

		<hist name='norm_circ' input='analyzeisolationMET/var_norm_circularity'>
			<var name='qcd' source='file:qcd' />
			<var name='qcdBG' source='file:qcdCircBG' color='2' />
			<var name='qcdSIG' source='file:qcdCircSIG' color='4' />
			<titleX v='circ' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/circ" />
		</hist>
		
		<hist name='norm_dPhiMuJ12' input='analyzeisolationMET/var_norm_DeltaPhiMuonJet12'>
			<var name='qcd' source='file:qcd' />
			<var name='qcdBG' source='file:qcdCircBG' color='2' />
			<var name='qcdSIG' source='file:qcdCircSIG' color='4' />
			<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/circ" />
		</hist>

		<hist name='norm_dPhiMETMu' input='analyzeisolationMET/var_norm_deltaPhiMetleadingMuon'>
			<var name='qcd' source='file:qcd' />
			<var name='qcdBG' source='file:qcdCircBG' color='2' />
			<var name='qcdSIG' source='file:qcdCircSIG' color='4' />
			<titleX v='\Delta\Phi(MET,\mu)' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/circ" />
		</hist>

		<hist name='norm_muon_pt' input='analyzeisolationMET/var_norm_muon_pt'>
			<var name='qcd' source='file:qcd' norm='Integral' />
			<var name='qcdBG' source='file:qcdCircBG' color='2' norm='Integral' />
			<var name='qcdSIG' source='file:qcdCircSIG' color='4' norm='Integral' />
			<titleX v='p_t(\mu)' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/circ" />
		</hist>

		<hist name='norm_mva' input='analyzeisolationMET/var_MVAdisc'>
			<var name='qcd' source='file:qcd' norm='Integral' />
			<var name='qcdBG' source='file:qcdCircBG' color='2' norm='Integral' />
			<var name='qcdSIG' source='file:qcdCircSIG' color='4' norm='Integral' />
			<titleX v='MVA disc.' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/circ" />
		</hist>

		<!--		PT-->
		<hist name='weights'>
			<var name='bg-like' source='file:macro' input="shapes/ptBG" color='2' />
			<var name='sig.-like' source='file:macro' input="shapes/ptSIG" color='4' />
			<titleX v='p_{T}(#mu)' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/pt" />
			<legend></legend>
		</hist>
		
		<hist name='quality_tight' >
			<var name='qcd' source='file:macro' input="applied/jetcut_20/all/bkg/quality_tight" size="3"/>
			<var name='qcdBG' source='file:macro' input="shapes/MuPtBG/all/bkg/quality_tight" color='2' drawOption="e4" type="filled" style="3005"/>
			<var name='qcdSIG' source='file:macro' input="shapes/MuPtSIG/all/bkg/quality_tight" color='4' drawOption="e4" type="filled" style="3004"/>
			<titleX v='MVA disc. cut' />
			<titleY v='N_{est}/N_{true}' />
			<maxY v="1.55" />
			<minY v="0.55" />
			<savefolder v="shapes/pt" />
		</hist>
		
		<hist name='quality_loose' >
			<var name='qcd' source='file:macro' input="applied/jetcut_20/all/bkg/quality_loose" size="3"/>
			<var name='qcdBG' source='file:macro' input="shapes/MuPtBG/all/bkg/quality_loose" color='2' drawOption="e4" type="filled" style="3005"/>
			<var name='qcdSIG' source='file:macro' input="shapes/MuPtSIG/all/bkg/quality_loose" color='4' drawOption="e4" type="filled" style="3004"/>
			<titleX v='MVA disc. cut' />
			<titleY v='N_{est}/N_{true}' />
			<maxY v="1.015" />
			<minY v="0.985" />
			<savefolder v="shapes/pt" />
		</hist>

		<hist name='norm_circ' input='analyzeisolationMET/var_norm_circularity'>
			<var name='qcd' source='file:qcd' />
			<var name='qcdBG' source='file:qcdPtBG' color='2' />
			<var name='qcdSIG' source='file:qcdPtSIG' color='4' />
