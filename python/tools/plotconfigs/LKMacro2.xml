			<var name='qcdDown' source='file:qcdMJ12SIG' color='4' norm='Integral' />
			<titleX v='p_{T}(\mu) [GeV]' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/MJ12" />
		</hist>

