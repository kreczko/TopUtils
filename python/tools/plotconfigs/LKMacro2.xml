			<var name='qcdUp' source='file:qcdMETMuBG' color='2' />
			<var name='qcdDown' source='file:qcdMETMuSIG' color='4' />
			<titleX v='\Delta\Phi(MET,\mu)' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/METMu" />
		</hist>

		<hist name='norm_muon_pt' input='analyzeisolationMET/var_norm_muon_pt'>
			<var name='qcd' source='file:qcd' norm='Integral' />
			<var name='qcdUp' source='file:qcdMETMuBG' color='2' norm='Integral' />
			<var name='qcdDown' source='file:qcdMETMuSIG' color='4' norm='Integral' />
			<titleX v='p_{T}(\mu) [GeV]' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/METMu" />
		</hist>

		<hist name='norm_MVA'>
		<var name='central' source='file:qcd' input="allmbefore/mbg_nVSdisc" norm='Integral' />
			<var name='down-weighted' source='file:qcdMETMuBG' input="analyzeisolationMET/var_MVAdisc" color='2' norm='Integral' />
			<var name='up-weighted' source='file:qcdMETMuSIG' input="analyzeisolationMET/var_MVAdisc" color='4' norm='Integral' />
			<legend sizeX="45" posY="70" posX="30"/>
<!--			<var name='qcd' source='file:qcd' norm='Integral' />-->
<!--			<var name='qcdUp' source='file:qcdMETMuBG' color='2' norm='Integral' />-->
<!--			<var name='qcdDown' source='file:qcdMETMuSIG' color='4' norm='Integral' />-->
			<titleX v='d_{MVA}' />
			<titleY v='normalized to unit area' />
			<savefolder v="shapes/METMu" />
		</hist>
	</plots>
</HistPlotter>