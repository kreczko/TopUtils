			<var name='qcdUp' source='file:qcdMJ12BG' color='2' />
			<var name='qcdDown' source='file:qcdMJ12SIG' color='4' />
			<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
			<titleY v='normalized to unit area' />
