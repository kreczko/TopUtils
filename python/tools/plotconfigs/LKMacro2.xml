		<!--		MET MU-->
		<hist name='weights'>
			<var name='bg-like' source='file:macro' input="shapes/METMuBG" color='2' />
			<var name='sig-like' source='file:macro' input="shapes/METMuSIG" color='4' />
			<titleX v='weight factor' />
			<titleY v='normalized to unit area' />
			<legend posX="25" />
			<savefolder v="shapes/METMu" />
		</hist>
		
		<hist name='quality_tight' >
			<var name='qcd' source='file:macro' input="applied/jetcut_20/all/bkg/quality_tight" size="3"/>
			<var name='qcdBG' source='file:macro' input="shapes/dPhiMETMuBG/all/bkg/quality_tight" color='2' drawOption="e4" type="filled" style="3005"/>
