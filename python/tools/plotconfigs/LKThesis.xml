<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- rule declaration possible -->
	<!-- http://www.devmag.net/xml/xml_schema_elemente_attribute.htm -->
	<!-- final steering file -->
<HistPlotter>
	<include file='plotconfigs/MuonAnalyzerPlots.xml' />
	<include file='plotconfigs/IsolationAnalyzerPlots.xml' />
	<include file='plotconfigs/JetAnalyzerPlots.xml' />
	<sources>
		<!-- gets input from 1 canvogram -->
		<input name="ecalIsoPt">
			<folder name='analyzeSelMuons'>
				<filter type='exact' value='iso_dREcalPt' />
			</folder>
		</input>
		<input name="hcalIsoPt">
			<folder name='analyzeSelMuons'>
			<filter type='exact' value='iso_dRHcalPt' />
			</folder>
		</input>

		<!-- includes of the data -->
		<file id='qcd' name='{$QCD}' />
		<file id='top' name='{$TOP}' />
		<file id='wjets' name='{$WJETS}' />
<!--		remove them:-->
		<file id='qcd20' name='/playground/rootfiles/FINAL/qcdmu_20j20m.root' />
		<file id='qcdMET' name='/playground/rootfiles/FINAL/shapes/qcdMu_MJ12SIG.root' />
		<file id='top20' name='/playground/rootfiles/FINAL/top_20j20m.root' />
		<file id='wjets20' name='/playground/rootfiles/FINAL/wjets_20j20m.root' />

	</sources>
	<!--
		makes the canvas, mixes the canvogramms defined in the canvSetup summaryFile
		= ps oder pdf
	-->
	<plots create='png' makeSummaryFile='true' summaryFile='inspect.pdf'>
