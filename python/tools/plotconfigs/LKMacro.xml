			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='#epsilon_{QCD}' />
			<showErrors v='true' />
			<!--			<maxY v="0.018" />-->
			<maxY v="0.032" />
			<legend posX="20" sizeX="60" posY="40" sizeY="5"></legend>
		</hist>

		<hist name='eff_qcd' input='bkg/effvsDiscCut_BG'>
			<var name='QCD(#mu enriched)' source='file:macro, input:app20' color='2' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='#epsilon_{QCD}^{est.}' />
			<!--			<legend posX='50' />-->
			<showErrors v='true' />
		</hist>

		<hist name='eff_qcd5' input='bkg/effvsDiscCut_BG'>
			<var name='qcd' source='file:QCD5, input:app20' color='2' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='#epsilon_{QCD}^{est.}' />
			<!--			<legend posX='50' />-->
			<showErrors v='true' />
		</hist>

