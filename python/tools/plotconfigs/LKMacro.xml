<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- rule declaration possible -->
	<!-- http://www.devmag.net/xml/xml_schema_elemente_attribute.htm -->
	<!-- final steering file -->
<HistPlotter>
	<sources>
		<!-- gets input from 1 canvogram -->
		<input name='qcd20'>
			<folder name="qcd/jetcut_20" />
		</input>
		<input name='top20'>
			<folder name="top/jetcut_20" />
		</input>
		<input name='wjets20'>
			<folder name="wjets/jetcut_20" />
		</input>
		<input name='mixed20'>
			<folder name="mixed/jetcut_20" />
		</input>
		<input name='jcuts'>
			<folder name='applied/Jcuts' />
		</input>

		<input name='mcuts'>
			<folder name='applied/Mcuts' />
		</input>
		<input name='app20'>
			<folder name="applied/jetcut_20/all" />
		</input>
		<input name='app25'>
			<folder name="applied/jetcut_25/all" />
		</input>
		<input name='app30'>
			<folder name="applied/jetcut_30/all" />
		</input>
		<input name='app35'>
			<folder name="applied/jetcut_35/all" />
		</input>
		<input name='app40'>
			<folder name="applied/jetcut_40/all" />
		</input>

		<input name='app20m'>
			<folder name="applied/muoncut_20/all" />
		</input>
		<input name='app22.5m'>
			<folder name="applied/muoncut_22.5/all" />
		</input>
		<input name='app25m'>
			<folder name="applied/muoncut_25/all" />
		</input>
		<input name='app27.5m'>
			<folder name="applied/muoncut_27.5/all" />
		</input>
		<input name='app30m'>
			<folder name="applied/muoncut_30/all" />
		</input>

		<file id="macro" name="{$macro}" />
		<file id="QCD2" name="/playground/rootfiles/FINAL/complete_canvs_MM_2QCD.root" />
		<file id="wjets2" name="/playground/rootfiles/FINAL/complete_canvs_MM_2wjet.root" />
		<file id="top2" name="/playground/rootfiles/FINAL/complete_canvs_MM_2wjet.root" />
		<file id="sig2" name="/playground/rootfiles/FINAL/complete_canvs_MM_2sig.root" />
		<file id="QCD5" name="/playground/rootfiles/FINAL/complete_canvs_MM_5QCD.root" />
	</sources>
	<plots create='png' makeSummaryFile='true' summaryFile='inspect.pdf'>
		<!-- Macro plots -->
		<canv name='NfractionVSMVACut'>
			<hist name='loose' source='file:macro' input="applied/muoncut_20/all/bkg/NvsCutloose"
				color='2' />
			<hist name='tight' source='file:macro' input="applied/muoncut_20/all/bkg/NvsCuttight"
				color='4' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v="N_{QCD}(d_{MVA} &lt; d_{MVA}^{thresh.})/N_{QCD}" />
			<legend posX="40" posY="30" />
			<minY v="0" />
		</canv>

		<canv name='NfractionVSMVACut_M30'>
			<hist name='loose' source='file:macro' input="applied/muoncut_30/all/bkg/NvsCutloose"
				color='2' />
			<hist name='tight' source='file:macro' input="applied/muoncut_30/all/bkg/NvsCuttight"
				color='4' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v="N_{QCD}(d_{MVA}^{thresh.})/N_{QCD}" />
			<legend posX="20" posY="60" />
		</canv>

		<canv name='NfractionVSMVACut_J40'>
			<hist name='loose' source='file:macro' input="applied/jetcut_40/all/bkg/NvsCutloose"
				color='2' />
			<hist name='tight' source='file:macro' input="applied/jetcut_40/all/bkg/NvsCuttight"
				color='4' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v="N(d_{MVA}^{thresh.})/N_{QCD}" />
			<legend posX="40" posY="30" />
		</canv>

		<canv name='bbs_QCD'>
			<hist name='loose' source='file:macro' input="applied/muoncut_20/all/bkg/bgOverbgs_loose"
				color="2" />
			<hist name='tight' source='file:macro' input="applied/muoncut_20/all/bkg/bgOverbgs_tight"
				color="4" />
			<titleX v='d_{MVA}' />
			<titleY v='B/(B+S)' />
			<legend posX="20" posY="30" />
			<minY v="0" />
		</canv>

		<canv name='bbs_QCD5'>
			<hist name='QCD(#mu enriched)' source='file:macro' input="applied/muoncut_20/all/bkg/bgOverbgs_tight"
				 />
			<!--
				<hist name='tight' source='file:QCD5'
				input="applied/muoncut_20/all/bkg/bgOverbgs_tight"
			-->
			<!--				color="2" />-->
			<hist name='tmp1' source='file:QCD5' input="qcd/muoncut_20/all/nVSdiscTight"
				draw="0" />
			<hist name='tmp2' source='file:QCD5' input="mixed/muoncut_20/all/nVSdiscTight"
				draw="0" />
			<hist name='QCD(#mu enriched) x 5' source='hist:tmp1, hist:tmp2' operation="div" color="2" />
			<titleX v='d_{MVA}' />
			<titleY v='B/(B+S)' />
			<legend posX="20" posY="10" sizeX="50"/>
			<minY v="0" />
		</canv>

		<canv name='bbs_QCD_J40'>
			<hist name='loose' source='file:macro' input="applied/jetcut_40/all/bkg/bgOverbgs_loose"
				color="2" />
			<hist name='tight' source='file:macro' input="applied/jetcut_40/all/bkg/bgOverbgs_tight"
				color="4" />
			<titleX v='d_{MVA}' />
			<titleY v='B/(B+S)' />
			<minY v="0" />
		</canv>

		<canv name='bbs_QCD_M30'>
			<hist name='loose' source='file:macro' input="applied/muoncut_30/all/bkg/bgOverbgs_loose"
				color="2" />
			<hist name='tight' source='file:macro' input="applied/muoncut_30/all/bkg/bgOverbgs_tight"
				color="4" />
			<titleX v='d_{MVA}' />
			<titleY v='B/(B+S)' />
			<minY v="0" />
		</canv>
		
		<canv name='quality_QCDLoose' input='bkg/quality_loose'>
			<hist name='QCD(#mu enriched)' source='file:macro, input:app20' color='2' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='N_{est}/N_{true}' />
			<legend posX='20' sizeX="50" posY="50"/>
					<maxY v='1.4' />	
					<minY v='0.9' />		
			<showErrors v='true' />
		</canv>
		
		<canv name='quality_QCDTight' input='bkg/quality_tight'>
			<hist name='QCD(#mu enriched)' source='file:macro, input:app20' color='2' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='N_{est}/N_{true}' />
			<legend posX='20' sizeX="50"  posY="74"/>
			<!--		<maxY v='1.01' />-->
			<!--		<minY v='1.006' />	-->
			<showErrors v='true' />
		</canv>
		<canv name='quality_QCDLoose_5' input='bkg/quality_loose'>
			<hist name='qcd' source='file:QCD5, input:app20' color='2' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='N_{est}/N_{true}' />
			<!--		<maxY v='1.01' />-->
			<!--		<minY v='1.006' />	-->
			<showErrors v='true' />
		</canv>
		<canv name='quality_SIGLoose' input='sig/quality_loose'>
			<hist name='signal' source='file:macro, input:app20' color='4' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='N_{est}/N_{true}' />
			<legend posX='50' sizeX="30" />
			<!--		<maxY v='0.2' />-->
			<!--		<minY v='-0.2' />		-->
			<showErrors v='true' />
		</canv>
		
		<canv name='eff_MC_cut' input="applied/jetcut_20/all/bkg/effMC_vs_MVAcut" >
			<hist name='qcd' source="file:macro" color="2"/>
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='true #epsilon_{QCD}' />
		</canv>
		
		<canv name='quality_SIGTight' input='sig/quality_tight'>
			<hist name='signal' source='file:macro, input:app20' color='4' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='N_{est}/N_{true}' />
			<legend posX='50' sizeX="30" />
			<!--		<maxY v='0.2' />-->
			<!--		<minY v='-0.2' />		-->
			<showErrors v='true' />
		</canv>

		<canv name='quality_qcd_eff_5' input='bkg/quality_eff_BG'>
			<hist name='QCD(#mu enriched) x 5'  source='file:QCD5, input:app20' color='2' />
			<hist name='QCD(#mu enriched)' source='file:macro, input:app20' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='#epsilon_{QCD, data} / #epsilon_{QCD, true}' />
			<showErrors v='true' /><legend posX="25" posY="50" sizeX="50"></legend>
		</canv>
		
		<canv name='quality_qcd_eff_both'>
		<hist name='from data' source='file:macro'
				input='applied/muoncut_20/all/bkg/effvsDiscCut_BG' />
			<hist name='from simulation' source='file:macro'
				input='applied/muoncut_20/all/bkg/effMC_vs_MVAcut' color='2' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='#epsilon_{QCD}' />
			<showErrors v='true' />
			<legend posX="30" sizeX="60" posY="20" sizeY="30"/>
		</canv>
		<canv name='masterPlotAll' input='all/efficiency'>
			<hist name='QCD(#mu enriched)' source='file:macro, input:qcd20' color='2' />
			<hist name='Top(signal)' source='file:macro, input:top20' color='4' />
			<hist name='W+jets' source='file:macro, input:wjets20' color='8' />
			<hist name='combined' source='file:macro, input:mixed20' />
			<titleX v='d_{MVA}' />
			<titleY v='isolation efficiency' />
			<legend  posY="45" posX="50"/>
			<showErrors v='true' />
		</canv>

		<canv name='masterPlotTrk' input='track/efficiency'>
			<hist name='qcd' source='file:macro, input:qcd20' color='2' />
			<hist name='top' source='file:macro, input:top20' color='4' />
			<hist name='wjets' source='file:macro, input:wjets20' color='8' />
			<hist name='combined' source='file:macro, input:mixed20'
				color='24' />
			<titleX v='d_{MVA}' />
			<titleY v='isolation efficiency' />
			<legend posY="60" />
			<showErrors v='true' />
		</canv>

		<canv name='eff_MC_qcd' input='all/efficiency'>
			<hist name='qcd' source='file:macro, input:qcd20' color='2' />
