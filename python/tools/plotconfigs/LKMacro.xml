		<!-- Cut plots -->
		<hist name='quality_QCDLoose_Jcuts' input='bkg/quality_loose'>
			<var name='jetCut 20' source='file:macro, input:app20' type='marker'
				color='2' style='20' />
			<var name='jetCut 25' source='file:macro, input:app25' type='marker'
				color='4' style='21' />
			<var name='jetCut 30' source='file:macro, input:app30' type='marker'
				color='6' style='22' />
			<var name='jetCut 35' source='file:macro, input:app35' type='marker'
				color='8' style='23' />
			<var name='jetCut 40' source='file:macro, input:app40' type='marker'
				style='27' />
			<titleX v='mva disc. cut' />
			<titleY v='N_{est}/N_{true}' />
<!--			<legend posX='20' />-->
		</hist>

		<hist name='bbs_QCDLoose_Jcuts' input='bkg/bgOverbgs_loose'>
			<var name='jetCut 20' source='file:macro, input:app20' type='marker'
				color='2' style='20' />
			<var name='jetCut 25' source='file:macro, input:app25' type='marker'
				color='4' style='21' />
			<var name='jetCut 30' source='file:macro, input:app30' type='marker'
				color='6' style='22' />
			<var name='jetCut 35' source='file:macro, input:app35' type='marker'
				color='8' style='23' />
			<var name='jetCut 40' source='file:macro, input:app40' type='marker'
				style='27' />
			<titleX v='mva disc.' />
			<titleY v='B/(B+S)' />
<!--			<legend posX='20' posY='40' />-->
		</hist>

		<hist name='bbs_QCDTight_Jcuts' input='bkg/bgOverbgs_tight'>
			<var name='jetCut 20' source='file:macro, input:app20' type='marker'
				color='2' style='20' />
			<var name='jetCut 25' source='file:macro, input:app25' type='marker'
				color='4' style='21' />
			<var name='jetCut 30' source='file:macro, input:app30' type='marker'
				color='6' style='22' />
			<var name='jetCut 35' source='file:macro, input:app35' type='marker'
				color='8' style='23' />
			<var name='jetCut 40' source='file:macro, input:app40' type='marker'
				style='27' />
			<titleX v='mva disc.' />
			<titleY v='B/(B+S)' />
<!--			<legend posX='70' />-->
		</hist>

		<hist name='quality_QCDTight_Jcuts' input='bkg/quality_tight'>
			<var name='E_{T}(jet 1-4) &gt; 20' source='file:macro, input:app20'
				type='marker' color='2' style='20' />
			<var name='E_{T}(jet 1-4) &gt; 25' source='file:macro, input:app25'
				type='marker' color='4' style='21' />
			<var name='E_{T}(jet 1-4) &gt; 30' source='file:macro, input:app30'
				type='marker' color='6' style='22' />
			<var name='E_{T}(jet 1-4) &gt; 35' source='file:macro, input:app35'
				type='marker' color='8' style='23' />
			<var name='E_{T}(jet 1-4) &gt; 40' source='file:macro, input:app40'
				type='marker' style='27' />
			<titleX v='mva disc. cut' />
			<titleY v='N_{est}/N_{true}' />
			<legend posX='20' sizeX="40" posY="50"/>
		</hist>

		<hist name='quality_QCDLoose_Mcuts' input='bkg/quality_loose'>
			<var name='p_{T}(#mu) > 20' source='file:macro, input:app20m' type='marker'
				color='2' style='20' />
			<var name='p_{T}(#mu) > 22.5' source='file:macro, input:app22.5m' type='marker'
				color='4' style='21' />
			<var name='p_{T}(#mu) > 25' source='file:macro, input:app25m' type='marker'
				color='6' style='22' />
			<var name='p_{T}(#mu) > 27.5' source='file:macro, input:app27.5m' type='marker'
				color='8' style='23' />
			<var name='p_{T}(#mu) > 30' source='file:macro, input:app30m' type='marker'
				style='27' />
			<titleX v='mva disc. cut' />
			<titleY v='N_{est}/N_{true}' />
<!--			<legend posX='20' sizeX="40" posY="50" />-->
		</hist>

		<hist name='quality_QCDTight_Mcuts' input='bkg/quality_tight'>
			<var name='p_{T}(#mu) > 20' source='file:macro, input:app20m' type='marker'
				color='2' style='20' />
			<var name='p_{T}(#mu)  > 22.5' source='file:macro, input:app22.5m' type='marker'
				color='4' style='21' />
			<var name='p_{T}(#mu) > 25' source='file:macro, input:app25m' type='marker'
				color='6' style='22' />
			<var name='p_{T}(#mu)  > 27.5' source='file:macro, input:app27.5m' type='marker'
				color='8' style='23' />
			<var name='p_{T}(#mu)  > 30' source='file:macro, input:app30m' type='marker'
				style='27' />
			<titleX v='mva disc. cut' />
			<titleY v='N_{est}/N_{true}' />
			<legend posX='20' sizeX="40" posY="50" />
		</hist>

		<hist name='bbs_QCDLoose_Mcuts' input='bkg/bgOverbgs_loose'>
			<var name='muon p_T > 20' source='file:macro, input:app20m' type='marker'
				color='2' style='20' />
			<var name='muon p_T > 22.5' source='file:macro, input:app22.5m' type='marker'
				color='4' style='21' />
			<var name='muon p_T > 25' source='file:macro, input:app25m' type='marker'
				color='6' style='22' />
			<var name='muon p_T > 27.5' source='file:macro, input:app27.5m' type='marker'
				color='8' style='23' />
			<var name='muon p_T > 30' source='file:macro, input:app30m' type='marker'
				style='27' />
			<titleX v='mva disc.' />
			<titleY v='B/(B+S)' />
			<legend posX='20' posY='40' />
		</hist>

		<hist name='bbs_QCDTight_Mcuts' input='bkg/bgOverbgs_tight'>
			<var name='muon p_T > 20' source='file:macro, input:app20m' type='marker'
				color='2' style='20' />
			<var name='muon p_T > 22.5' source='file:macro, input:app22.5m' type='marker'
				color='4' style='21' />
			<var name='muon p_T > 25' source='file:macro, input:app25m' type='marker'
				color='6' style='22' />
			<var name='muon p_T > 27.5' source='file:macro, input:app27.5m' type='marker'
				color='8' style='23' />
			<var name='muon p_T > 30' source='file:macro, input:app30m' type='marker'
				style='27' />
			<titleX v='mva disc.' />
			<titleY v='B/(B+S)' />
			<legend posX='70' />
		</hist>
	</plots>
</HistPlotter>