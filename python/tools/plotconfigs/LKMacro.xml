			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='#epsilon_{QCD}' />
			<showErrors v='true' />
			<!--			<maxY v="0.018" />-->
			<maxY v="0.032" />
			<legend posX="20" sizeX="60" posY="50"></legend>
		</canv>

		<canv name='eff_qcd' input='bkg/effvsDiscCut_BG'>
			<hist name='QCD(#mu enriched)' source='file:macro, input:app20' color='2' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='#epsilon_{QCD}^{est.}' />
			<!--			<legend posX='50' />-->
			<showErrors v='true' />
		</canv>

		<canv name='eff_qcd5' input='bkg/effvsDiscCut_BG'>
			<hist name='qcd' source='file:QCD5, input:app20' color='2' />
			<titleX v='d_{MVA}^{thresh.}' />
			<titleY v='#epsilon_{QCD}^{est.}' />
			<!--			<legend posX='50' />-->
			<showErrors v='true' />
		</canv>

				<canvlist name='QCD' input='jcuts'>
					<hist name='QCD(#mu enriched)' source='file:macro' color='2' />
					<titleX v='E_{T}(jet) cut [GeV]' />
					<titleY v='N_{est}/N_{true}' />
							<maxY v='1.4' />
							<minY v='0' />
					<showErrors v='true' />
					<legend posX='20' posY="30" />
				</canvlist>

				<canvlist name='QCD' input='mcuts'>
