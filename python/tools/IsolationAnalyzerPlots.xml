<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- Works with LKThesis.xml -->
<HistSetup>
	<hist name='MVAdisc_signal' input='analyzeisolationMET/var_MVAdisc'>
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' draw='0' />
		<var name='top' source='file:top' type='line' style='1' color='4' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8' />
		<titleX v='MVA disc.' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
		<legend name='plotname' fonsize='12' posX='40' />
	</hist>
	<hist name='MVAdisc_qcd' input='analyzeisolationMET/var_MVAdisc'>
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' />
		<var name='top' source='file:top' type='line' style='1' color='4' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8' />
		<titleX v='MVA disc.' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
		<legend name='plotname' fonsize='12' posX='40' />
	</hist>

	<hist name='norm_MVAdisc' input='analyzeisolationMET/var_MVAdisc'>
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' norm='Integral' />
		<var name='top' source='file:top' type='line' style='1' color='4' norm='Integral' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8'
			norm='Integral' />
		<titleX v='MVA disc.' />
		<titleY v='AU' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
		<legend name='plotname' fonsize='12' posX='40' />
	</hist>

	<hist name='var_norm_dPhiMuJ12' input='analyzeisolationMET/var_norm_DeltaPhiMuonJet12'>
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' />
		<var name='top' source='file:top' type='line' style='1' color='4' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8' />
		<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
		<titleY v='AU' />
		<!--			<logY v='1' />-->
		<savefolder v='analysis' />
		<legend name='plotname' fonsize='12' posX='40' />
	</hist>
	
	<hist name='corr_DphiMuJ1J2VScalo_qcd' input='analyzeisolationMET/corr_DeltaPhiMuonJet12VScalo'>
		<!-- name is also used for legend -->
		<var name='wjets' source='file:qcd' type='line' style='1' color='2' />
		<titleY v='calo iso. [GeV]' />
		<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend name='plotname' fonsize='12' />
	</hist>
	
	<hist name='corr_DphiMuJ1J2VStrack_qcd' input='analyzeisolationMET/corr_DeltaPhiMuonJet12VStrack'>
		<!-- name is also used for legend -->
		<var name='wjets' source='file:qcd' type='line' style='1' color='2' />
		<titleY v='track iso. [GeV]' />
		<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend name='plotname' fonsize='12' />
	</hist>
	
	<hist name='corr_DphiMuJ1J2VSjetIso_qcd' input='analyzeisolationMET/corr_DeltaPhiMuonJet12VSjetiso'>
		<!-- name is also used for legend -->
		<var name='wjets' source='file:qcd' type='line' style='1' color='2' />
		<titleY v='distance to closest jet' />
		<titleX v='\Delta\phi(\mu, jet 1) + \Delta\phi(\mu, jet 2)' />
		<drawOption v="COLZ" />
		<savefolder v='corr' />
		<legend name='plotname' fonsize='12' />
	</hist>
	
