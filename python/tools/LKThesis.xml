<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- rule declaration possible -->
	<!-- http://www.devmag.net/xml/xml_schema_elemente_attribute.htm -->
	<!-- final steering file -->
<HistPlotter>
	<include file='MuonAnalyzerPlots.xml' />
	<include file='IsolationAnalyzerPlots.xml' />
	<include file='JetAnalyzerPlots.xml' />
	<include file='MatrixAnalyzerPlots.xml' />
	<sources>
		<!-- gets input from 1 histogram -->
		<input name="ecalIsoPt">
			<folder name='analyzeSelMuons'>
				<filter type='exact' value='iso_dREcalPt' />
			</folder>
		</input>
		<input name="hcalIsoPt">
			<folder name='analyzeSelMuons'>
			<filter type='exact' value='iso_dRHcalPt' />
			</folder>
		</input>

		<!-- includes of the data -->
		<file id='qcd' name='{$QCD}' />
		<file id='top' name='{$TOP}' />
		<file id='wjets' name='{$WJETS}' />
		<file id='qcd20' name='/playground/rootfiles/FINAL/MM_qcdmu_270109.root' />
		<file id='top20' name='/playground/rootfiles/FINAL/MM_top_270109.root' />
		<file id='wjets20' name='/playground/rootfiles/FINAL/MM_wjets_270109.root' />

	</sources>
	<!--
		makes the canvas, mixes the histogramms defined in the histSetup summaryFile
		= ps oder pdf
	-->
	<plots create='eps,pdf' makeSummaryFile='true' summaryFile='inspect.pdf'>
		<!-- IsolationAnalyzer Plots -->
		<hist setup='MVAdisc_signal' />
		<hist setup='MVAdisc_qcd' />
		<hist setup="norm_MET" />
		<hist setup='var_norm_dPhiMuJ12' />
		<hist setup='corr_DphiMuJ1J2VScalo_qcd' />
		<hist setup='corr_DphiMuJ1J2VStrack_qcd' />
		<hist setup='corr_DphiMuJ1J2VSjetIso_qcd' />		
		
		<hist setup='var_norm_dPhiMETMu' />
		<hist setup='corr_dPhiMETMuVScalo_qcd' />
		<hist setup='corr_dPhiMETMuVStrack_qcd' />
		<hist setup='corr_dPhiMETMuVSjetIso_qcd' />
		
		<hist setup='var_norm_Circ' />
		<hist setup='norm_MVAdisc' />
		<hist setup='sumPhiMuJ1J2vsDphiJ1J2_qcd' />
		<hist setup='corr_MVAdiscVScalo_qcd' />
		<hist setup='corr_MVAdiscVSjetIso_qcd' />
		<hist setup='corr_MVAdiscVStrack_qcd' />

		<hist setup='sumPhiMuJ1J2vsDphiJ1J2_top' />
		<hist setup='corr_MVAdiscVScalo_top' />
		<hist setup='corr_MVAdiscVSjetIso_top' />
		<hist setup='corr_MVAdiscVStrack_top' />

		<hist setup='sumPhiMuJ1J2vsDphiJ1J2_wjets' />
		<hist setup='corr_MVAdiscVScalo_wjets' />
		<hist setup='corr_MVAdiscVSjetIso_wjets' />
		<hist setup='corr_MVAdiscVStrack_wjets' />

		<!-- MuonAnalyzer Plots -->
		<hist setup='muon_pt' />
		<hist setup='muon_calIso' />
		<hist setup='muon_PtTracksVSdR' />
		<hist setup='muon_NTracksVSdR' />
		<hist setup='muon_isoJet05' />
		<hist setup='muon_isoJet10' />
		<hist setup='muon_isoJet15' />
		<hist setup='muon_isoJet20' />
		<hist setup='muon_isoJet30' />
		<hist setup='muon_isoJet35' />
		<hist setup='muon_isoJet40' />
		<hist setup='var_norm_dPhiMuJ12' />
		<hist setup='muon_eta' />
		<hist setup='muon_calomix' />
		<hist setup='muon_trackIso' />
		

		<!-- JetAnalyzer plots -->
		<hist setup='jet1_et' />
		<hist setup='jet2_et' />
		<hist setup='jet3_et' />
		<hist setup='jet4_et' />
		<hist setup='nJets20' />
		
	</plots>
</HistPlotter>