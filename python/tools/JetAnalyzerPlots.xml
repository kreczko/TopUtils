	<!-- Works with LKThesis.xml -->
<HistSetup>

	<hist name='jet1_et' input='analyzeSelJets/kin_e_0'>
		<!-- name is also used for legend -->
		<var name='qcd' source='file:qcd' type='line' style='1' color='2'
			norm='Integral' />
		<var name='top' source='file:top' type='line' style='1' color='4'
			norm='Integral' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8'
			norm='Integral' />
		<!--
			<var name='signal' source='var:top,var:wjets' operation='add'
			type='filled'
		-->
		<!--				style='3005'  color='6' norm='Integral' />-->
		<titleX v='jet 1 E_{T} [GeV]' />
		<titleY v='AU' />
		<savefolder v='analysis' />
		<legend name='plotname' fonsize='12' />
	</hist>
