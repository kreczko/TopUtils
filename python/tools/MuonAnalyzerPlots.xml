<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
	<!-- Works with LKThesis.xml -->
<HistSetup>
	<!-- From MuonAnalyzer -->
	<hist name='muon_pt' input='analyzeSelMuons/kin_pt'>
		<!-- name is also used for legend -->
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' norm='Integral' />
		<var name='top' source='file:top' type='line' style='1' color='4' norm='Integral' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8'
			norm='Integral' />
		<titleX v='p_{T} [GeV]' />
		<titleY v='AU' />
		<savefolder v='iso' />
		<legend name='plotname' fonsize='12' />
	</hist>
	<hist name='muon_calIso' input='analyzeSelMuons/iso_isoCalPt'>
		<!-- name is also used for legend -->
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' norm='Integral' />
		<var name='top' source='file:top' type='line' style='1' color='4' norm='Integral' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8'
			norm='Integral' />
		<titleX v='muon cal. iso. [GeV]' />
		<titleY v='AU' />
		<savefolder v='iso' />
		<legend posX='20' name='plotname' fonsize='12' />
	</hist>
	
		<hist name='muon_calomix' input=''>
		<var name='qcdE' source='file:qcd, input:ecalIsoPt' type='line' style='1' color='2' draw='0'  />
		<var name='topE' source='file:top, input:ecalIsoPt' type='line' style='1' color='4' draw='0' />
		<var name='wjetsE' source='file:wjets, input:ecalIsoPt' type='line' style='1' color='8' draw='0'  />
		<var name='qcdH' source='file:qcd, input:hcalIsoPt' type='line' style='1' color='2' draw='0'  />
		<var name='topH' source='file:top, input:hcalIsoPt' type='line' style='1' color='4' draw='0' />
		<var name='wjetsH' source='file:wjets, input:hcalIsoPt' type='line' style='1' color='8' draw='0'  />
		
		<var name='qcd' source='var:qcdE, var:qcdH' type='line' style='1' color='2' norm='Integral'  operation='add' />
		<var name='top' source='var:topE, var:topH' type='line' style='1' color='4' norm='Integral'  operation='add'/>
		<var name='wjets' source='var:wjetsH, var:wjetsE' type='line' style='1' color='8' norm='Integral' operation='add' />
		<titleX v='min. \Delta R(muon, caloTower)' />
		<titleY v='AU' />
		<savefolder v='iso' />
		<legend name='plotname' fonsize='12' />
	</hist>
	
	<hist name='muon_trackIso' input='analyzeSelMuons/iso_isoTrkPt'>
		<!-- name is also used for legend -->
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' norm='Integral' />
		<var name='top' source='file:top' type='line' style='1' color='4' norm='Integral' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8'
			norm='Integral' />
		<titleX v='muon track iso. [GeV]' />
		<titleY v='AU' />
		<savefolder v='iso' />
		<legend posX='20' name='plotname' fonsize='12' />
	</hist>

	<hist name='muon_PtTracksVSdR' input='analyzeSelMuons/iso_dRTrkPt'>
		<!-- name is also used for legend -->
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' />
		<var name='top' source='file:top' type='line' style='1' color='4' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8' />
		<titleX v='\Delta R(muon, track)' />
		<titleY v='p_{T} of tracks' />
		<savefolder v='iso' />
		<legend posX='20' name='plotname' fonsize='12' />
	</hist>
	<hist name='muon_NTracksVSdR' input='analyzeSelMuons/iso_dRTrkN'>
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' />
		<var name='top' source='file:top' type='line' style='1' color='4' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8' />
		<titleX v='\Delta R(muon, track)' />
		<titleY v='# of tracks' />
		<savefolder v='iso' />
		<legend posX='20' name='plotname' fonsize='12' />
	</hist>
	<hist name='muon_isoJet20' input='analyzeSelMuons/iso_isoJet20'>
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' norm='Integral' />
		<var name='top' source='file:top' type='line' style='1' color='4' norm='Integral' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8'
			norm='Integral' />
		<titleX v='min. \Delta R(muon, jets(E_{T} > 20 GeV) )' />
		<titleY v='AU' />
		<savefolder v='iso' />
		<legend name='plotname' fonsize='12' />
	</hist>

	<hist name='muon_eta' input='analyzeSelMuons/kin_eta'>
		<!-- name is also used for legend -->
		<var name='qcd' source='file:qcd' type='line' style='1' color='2' norm='Integral' />
		<var name='top' source='file:top' type='line' style='1' color='4' norm='Integral' />
		<var name='wjets' source='file:wjets' type='line' style='1' color='8'
			norm='Integral' />
		<titleX v='\eta' />
		<!--			<logY v='1'/>-->
		<titleY v='AU' />
		<savefolder v='kin' />
		<legend name='plotname' fonsize='12' />
	</hist>

</HistSetup>